<div class="container m-5">
  <h1>Lists</h1>
  <% @lists.each do |list| %>
  <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://www.mca.com.au/files/images/Hiroshi-Sugimoto-UA-Playhouse-.width-1900.jpegquality-85.jpg)">
    <%= link_to list.name, list_path(list), class: 'text-white' %>
  </div>
  <% end %>
  <hr>
  <%= link_to "Create new list", new_list_path, class: 'btn btn-primary' %>
</div>
